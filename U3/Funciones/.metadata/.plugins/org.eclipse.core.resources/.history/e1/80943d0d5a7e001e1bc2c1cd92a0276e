package main;

import java.util.Random;

public class App {

	public static void main(String[] args) {
		
		for (int i = 1; i <=10; i++) {
			System.out.println(dameCaracter());
		}
		
	}

	static char dameCaracter() {
		Random rnd = new Random();
		int codAscii = rnd.nextInt(26)+65;
		return (char) codAscii;
	}
}
